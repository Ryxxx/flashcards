enum Couleur {
  BLANC,
  NOIR
}

entity Ouverture {
	id Long,
    nom String,
    couleur Couleur
}

entity Variante {
	id Long,
    nom String,
    coups String
    
}

relationship OneToOne {
    Ouverture{userId} to User
}

relationship OneToMany {
	Ouverture{ouvertureId} to Variante
}

// Set service options to all except few
service all with serviceImpl